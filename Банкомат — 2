---Банкомат if:

#include <iostream>

int main() {
  
int cash;
  
  std::cout << "----------Банкомат----------\n";
  std::cout << "Сколько хотите вывести денег?: ";
  std::cin >> cash;

  if (cash > 150000)
    std::cout << "Error ";
  if (cash == 100) 
    std::cout << "Банкомат выдал 1 купюру по 100 рублей";
    else if (cash <= 100)
      std::cout << "Банкомат не может выдать такую сумму";
    
  if(cash == 200)
    std::cout << "Банкомат выдал 1 купюру по 200 рублей";
    else if (cash >= 101 && cash <= 199)
      std::cout << "Банкомат не может выдать такую сумму";
  
  if(cash == 500)
    std::cout << "Банкомат выдал 1 купюру по 500 рублей";
    else if (cash >= 201 && cash <= 499)
      std::cout << "Банкомат не может выдать такую сумму";
  
  if(cash == 1000)
    std::cout << "Банкомат выдал 1 купюру по 1000 рублей";
    else if (cash >= 501 && cash <= 999)
      std::cout << "Банкомат не может выдать такую сумму";
  
  if (cash == 2000)
    std::cout << "Банкомат выдал 1 купюру по 2000 рублей";
    else if (cash >= 1001 && cash <= 1999)
      std::cout << "Банкомат не может выдать такую сумму";
  if (cash == 5000)
    std::cout << "Банкомат выдал 1 купюру по 5000 рублей";
    else if (cash >= 2001 && cash <= 4999)
      std::cout << "Банкомат не может выдать такую сумму";

  
  else if (cash == 10000){
    int result = 100 * 100;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";  
  }
  else if (cash == 15000){
    int result = (1000 + 500) * 10;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
  else if (cash == 20000){
    int result = 200 * 100;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
  else if (cash == 25000){
    int result = (2000 + 500) * 100;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
  else if (cash == 50000){
    int result = 500 * 100;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
  else if (cash == 75000){
    int result = (5000 + 2000 + 500) * 100;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
  else if (cash == 100000){
    int result = 1000 * 100;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
  else if (cash == 125000){
    int result = (1000 + 200) * 100 + 500 * 10;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
  else if (cash == 150000){
    int result = (1000 + 500) * 100;
    std::cout << "Банкомат выдал " << result / 5000 << " купюр по 5000 рублей";
  }
}
